[mysql]

[mysqld]
sql-mode="NO_BACKSLASH_ESCAPES,STRICT_TRANS_TABLES,NO_ENGINE_SUBSTITUTION"
character-set-server=utf8mb4
default_authentication_plugin=mysql_native_password  # 由于此处是最新的mysql8.0,此处的加密模式变化,设置成这样就没问题